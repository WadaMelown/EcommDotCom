<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.S6105692">

    <!--
         The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but you must specify either coarse or fine
         location permissions for the 'MyLocation' functionality.
    -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key"
            />

        <activity
            android:name="com.example.S6105692.MapsActivity"
            android:label="@string/title_activity_maps"></activity>
        <activity android:name="com.example.S6105692.CreateListingActivity" />
        <activity android:name="com.example.S6105692.SearchListingActivity" />
        <activity android:name="com.example.S6105692.BestSellers" />
        <activity android:name="com.example.S6105692.AdListingScreen" />
        <activity android:name="com.example.S6105692.HomeActivity" />
        <activity android:name="com.example.S6105692.RegisterActivity" />
        <activity android:name="com.example.S6105692.LoginActivity" />
        <activity android:name="com.example.S6105692.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>